<form method="POST" action="/celebrities/<%= celebrity.id %>/?_method=PUT">
    <label for="name">Name</label>
    <input type="text" name="name" value="<%= celebrity.name %> ">
  
    <label for="dob">Birthdate</label>
    <input id="dob" type="text" name="dob" value="<%= celebrity.dob %> ">>

    <label for="sign_name">Sign</label>
    <input id="sign_name" type="text" name="sign_name" value="<%= celebrity.sign_name %> ">>
    
    <input type="submit">
  </form>