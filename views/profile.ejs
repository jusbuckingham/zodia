<div class="card" id="profileCard">
    <div class="card-header">
      <h2>Profile Info</h2>
    </div>
    <div class="card-body">
      <table class="table table-borderless table-sm">
        <tbody>
          <tr>
            <td>Name:</td>
            <td><%= name %></td>
          </tr>
          <tr>
            <td>Username:</td>
            <td><%= email %></td>
          </tr>
        </tbody>
      </table>
    </div>
  </div>